{
    "multipart": [
        {   "when": { "up": "true" },
            "apply": { "model": "wuestUtilities:blockGrassWall_Post" }
        },
        {   "when": { "north": "true" },
            "apply": { "model": "wuestUtilities:blockGrassWall_Side", "uvlock": true }
        },
        {   "when": { "east": "true" },
            "apply": { "model": "wuestUtilities:blockGrassWall_Side", "y": 90, "uvlock": true }
        },
        {   "when": { "south": "true" },
            "apply": { "model": "wuestUtilities:blockGrassWall_Side", "y": 180, "uvlock": true }
        },
        {   "when": { "west": "true" },
            "apply": { "model": "wuestUtilities:blockGrassWall_Side", "y": 270, "uvlock": true }
        }
    ]
}
